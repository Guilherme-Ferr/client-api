{
  "openapi": "3.0.0",
  "info": {
    "title": "API do Ecommerce V2",
    "description": "Essa API tem como objetivo o controle de acesso do usuario",
    "termsOfService": "http://localhost:3333/terms",
    "contact": {
      "email": "guilherme@primi.com.br"
    },
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "http://localhost:3333/",
      "description": "API de teste"
    }
  ],
  "paths": {
    "/login": {
      "post": {
        "summary": "Login de usuario",
        "description": "Essa rota sera responsavel por efetuar o login do usuario no sistema",
        "tags": ["Users"],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Login"
              },
              "examples": {
                "Client Credentials": {
                  "value": {
                    "email": "email@gmail.com.br",
                    "password": "1122334455"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "404": {
            "description": "Unauthorized"
          },
          "200": {
            "description": "Ok",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "$ref": "#/components/schemas/Login"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Login": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string"
          },
          "password": {
            "type": "string"
          }
        }
      }
    }
  }
}
